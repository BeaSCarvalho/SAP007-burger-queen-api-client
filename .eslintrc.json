{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "prefer-destructuring": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "no-plusplus":0,
        "no-alert":0,
        "no-tabs":0,
        "no-inner-declarations":0,
        "no-param-reassign":0,
        "react/react-in-jsx-scope": 0,
        "quotes": ["error", "double"],
        "react/prop-types":0
    }
}
